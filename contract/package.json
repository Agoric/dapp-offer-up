  "homepage": "https://github.com/agoric-labs/dapp-join-game#readme",
  "eslintConfig": {
    "extends": [
      "@agoric"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "arrowParens": "avoid",
    "singleQuote": true
  }
