{
  "name": "game-places-contract",
  "version": "0.1.0",
  "private": true,
  "description": "Game Asset Contract",
  "type": "module",
  "scripts": {
    "start:docker": "docker-compose up -d",
    "docker:logs": "docker-compose logs --tail 200 -f",
